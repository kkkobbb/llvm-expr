# 関数宣言

# 実体のない宣言のみのダミー関数
decl fnc getval:int(:int, :string);
# 以降標準ライブラリの関数
decl fnc puts:int(:string);
decl fnc putchar:int(:int);
decl fnc printf:int(:string, ...);
decl fnc atoi:int(:string);
decl fnc rand:int();
decl fnc srand:void(:int); # 本来はunsignedだがintで代用



putchar(0x42); # B
putchar(0x0A); # \n

puts("AAA");
puts("AAA");
puts("CCCCC");
printf("printf\n");

srand(12);
printf("%d\n", rand());
printf("%d\n", rand());
printf("%d\n", rand());
printf("%d\n", rand());

atoi("2");

